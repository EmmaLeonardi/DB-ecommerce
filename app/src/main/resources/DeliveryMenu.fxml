<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="430.0" prefWidth="489.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="db.ecommerce.controller.DeliveryController">
   <children>
      <Label text="Consegne">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Label>
      <ListView fx:id="lstvw_delivery_pending" prefHeight="363.0" prefWidth="490.0" />
      <BorderPane prefHeight="31.0" prefWidth="490.0">
         <right>
            <Button fx:id="btn_details" mnemonicParsing="false" onAction="#details" text="Dettagli consegna" BorderPane.alignment="CENTER" />
         </right>
         <left>
            <Button fx:id="btn_new_delivery" mnemonicParsing="false" onAction="#newDelivery" text="Nuova consegna" BorderPane.alignment="CENTER" />
         </left>
      </BorderPane>
   </children>
</VBox>
